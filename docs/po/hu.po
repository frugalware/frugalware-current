"options=('scriptlet')  _F_sourceforge_dirname=\"djvu\" "
"_F_sourceforge_broken_up2date=1 Finclude sourceforge url=\"http://djvulibre."
"djvuzone.org/\" source=(${source[@]} head_-n1.patch no-OPTS-FLAGS-thx.patch)"
