url="http://www.fastcgi.com/"
groups=('devel-extra')
archs=('i686')
source=(http://www.fastcgi.com/dist/"$pkgname"-"$pkgver".tar.gz)
up2date='lynx -dump "http://www.fastcgi.com/dist/" | grep "$pkgname-[0-9.]\+.tar.gz" | tail -n 1 | sed "s/.*$pkgname-\(.*\).tar.gz.*/\1/"'
