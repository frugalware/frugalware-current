pkgname=e17genmenu
pkgver=4.2.0
pkgrel=1
pkgdesc="Generates menu for E17."
depends=('engrave')
groups=('e17-extra' 'e17-apps')
archs=('i686')
Finclude sourceforge
sha1sums=('bfbfb1caaead9605fbd89e84a8160fce352d6290')
options=('nodocs')

build() {
	Fcd $pkgname
	./autogen.sh || Fdie
	Fbuild
}

