pkgname=dnstracer
pkgver=1.8
pkgrel=1
pkgdesc="Determines where a given Domain Name Server gets its information from"
url="http://www.mavetju.org/unix/dnstracer.php/"
depends=('glibc')
groups=('network-extra')
up2date="lynx -dump http://www.mavetju.org/unix/general.php | grep 'dnstracer.*Dnstracer' | sed -e 's/.*dnstracer v\([0-9]\.[0-9]\)\. Dnstracer.*/\1/'"
source=(http://www.mavetju.org/download/$pkgname-$pkgver.tar.gz)
# could not find official md5sum, made it myself
md5sums=('f7e4102b572d052ca95567a08d9dde0a')
archs=('i686' 'x86_64')

