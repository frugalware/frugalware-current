pkgname=rox-diff
pkgver=2.1.4
pkgrel=1
pkgdesc="This program displays a coloured context diff between two files."
url="http://www.kerofin.demon.co.uk/rox/diff.html"
license="GPL2"
up2date="lynx -dump $url|grep 'Diff [0-9\.]\+[0-9]'|tail -n 1|cut -d ' ' -f 8"
source=(http://www.kerofin.demon.co.uk/rox/Diff-$pkgver.tar.gz $pkgname)
sha1sums=('6024a85d131b4af0ada80dbf3d1d657ce830dc98' \
          '574d64dc45263541687ee69c4c2ddd5ce8f3081c')
groups=('rox-extra')
archs=('i686')
depends=('rox-clib')
_F_app_dir="Diff"
Finclude rox

# optimalization ok - i686

