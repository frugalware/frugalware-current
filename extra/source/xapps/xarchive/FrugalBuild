pkgname=xarchive
pkgver=0.2.8
extrapkgver=2
pkgrel=1
pkgdesc="XArchive is a GTK+ front-end for command line archiving tools."
url="http://xarchive.sourceforge.net/"
depends=('gtk+2')
groups=('xapps-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump http://xarchive.sourceforge.net/ | grep -m1 'Latest Version' | sed 's/.* //;s/://'|sed 's/-.//g'"
source=(http://ovh.dl.sourceforge.net/sourceforge/xarchive/$pkgname-$pkgver-$extrapkgver.tar.gz)
sha1sums=('cac5522726c7c488cb5779cc5ac58e69234fa436')

build()
{
	Fcd $pkgname-$pkgver-$extrapkgver
	Fbuild
}

# optimalization OK
