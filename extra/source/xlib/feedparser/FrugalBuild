pkgname=feedparser
pkgver=4.1
pkgrel=1
pkgdesc="Python RSS and Atom feeds parser."
url="http://www.feedparser.org/"
depends=('python')
groups=('xlib-extra')
archs=('i686')
up2date="lynx -dump http://sourceforge.net/project/showfiles.php?group_id=112328 | grep -m 1 feedparser | sed 's/.*rser \[//;s/ .*$//;s/^...//'"
source=(http://heanet.dl.sourceforge.net/feedparser/$pkgname-$pkgver.zip)
sha1sums=('3a7174e11c8c523ea461c9d47a80526ba29a6544')

build()
{
	Fmkdir usr/share/doc/$pkgname-$pkgver
	Fcd ../
	mv ./docs/ $Fdestdir/usr/share/doc/$pkgname-$pkgver/
	Fbuild
}

# optimalization ok
