pkgname=direnv
pkgver=2.32.3
pkgrel=1
pkgdesc='a shell extension that manages your environment'
archs=('x86_64')
groups=('apps-extra')
url='https://direnv.net'
depends=('glibc')
makedepends=('go')
_F_github_tag_v=y
Finclude github
sha1sums=('69aa9bdbab49e050c61d5b6e05a9c6978f9cebe3')

build() {
	Fcd
	Fexec go build -o $pkgname || Fdie
	Fmakeinstall PREFIX=/usr
}

# optimization OK
