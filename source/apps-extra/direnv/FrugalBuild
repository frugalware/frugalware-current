pkgname=direnv
pkgver=2.32.1
pkgrel=1
pkgdesc='a shell extension that manages your environment'
archs=('x86_64')
groups=('apps-extra')
url='https://direnv.net'
depends=('glibc')
makedepends=('go')
_F_github_tag_v=y
Finclude github
sha1sums=('415c6028e2ed0c338e39b753393e62bd4395c060')

build() {
	Fcd
	Fexec go build -o $pkgname || Fdie
	Fmakeinstall PREFIX=/usr
}

# optimization OK
