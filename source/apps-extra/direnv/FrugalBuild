pkgname=direnv
pkgver=2.32.2
pkgrel=1
pkgdesc='a shell extension that manages your environment'
archs=('x86_64')
groups=('apps-extra')
url='https://direnv.net'
depends=('glibc')
makedepends=('go')
_F_github_tag_v=y
Finclude github
sha1sums=('48b13b59d5ce590f76afb86efba6ccd3cca82854')

build() {
	Fcd
	Fexec go build -o $pkgname || Fdie
	Fmakeinstall PREFIX=/usr
}

# optimization OK
