pkgname=docx2txt
pkgver=0.3
pkgrel=1
pkgdesc="Extract text contents from .docx file."
_F_sourceforge_prefix="v"
_F_sourceforge_ext=".tgz"
Finclude sourceforge
depends=('perl')
groups=('apps-extra')
archs=('i686' 'x86_64')
sha1sums=('be0adc58eafd06b6dbb56282c0f2748f7185b85b')

build()
{
	Fcd
	Fmakeinstall INSTALLDIR=$Fdestdir/$Fprefix/bin
}
