pkgname=mob
pkgver=5.1.0
pkgrel=1
pkgdesc='Tool for smooth git handover.'
archs=('x86_64')
groups=('apps-extra')
url='https://mob.sh'
depends=('glibc')
makedepends=('go')
_F_github_tag_v=y
_F_github_author=remotemobprogramming
Finclude github
sha1sums=('e263b0245785eaa0d371d097ab683e5cf71e91d6')

build() {
	Fcd
	Fexec go build -o $pkgname || Fdie
	Fexerel usr/bin/$pkgname
}

# optimization OK
