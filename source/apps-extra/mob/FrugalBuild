pkgname=mob
pkgver=4.4.6
pkgrel=1
pkgdesc='Tool for smooth git handover.'
archs=('x86_64')
groups=('apps-extra')
url='https://mob.sh'
depends=('glibc')
makedepends=('go')
_F_github_tag_v=y
_F_github_author=remotemobprogramming
Finclude github
sha1sums=('20df3deeb29eda4c950074987f77b2e5cfa85afa')

build() {
	Fcd
	Fexec go build -o $pkgname || Fdie
	Fexerel usr/bin/$pkgname
}

# optimization OK
