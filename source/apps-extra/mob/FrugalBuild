# Compiling Time: 0.44 SBU
# Maintainer: DeX77 <dex+fw@dragonslave.de>

pkgname=mob
pkgver=5.3.3
pkgrel=1
pkgdesc='Tool for smooth git handover.'
archs=('x86_64')
groups=('apps-extra')
url='https://mob.sh'
depends=('glibc')
makedepends=('go')
_F_github_tag_v=y
_F_github_author=remotemobprogramming
Finclude github
sha1sums=('115d53afce5b78954b1a7d5183b536eddffcf95d')

build() {
	Fcd
	Fexec go build -o $pkgname || Fdie
	Fexerel usr/bin/$pkgname
}

# optimization OK
