pkgname=mob
pkgver=5.2.0
pkgrel=1
pkgdesc='Tool for smooth git handover.'
archs=('x86_64')
groups=('apps-extra')
url='https://mob.sh'
depends=('glibc')
makedepends=('go')
_F_github_tag_v=y
_F_github_author=remotemobprogramming
Finclude github
sha1sums=('76798f6363cf57fa0f75032c6e135c770449a8c9')

build() {
	Fcd
	Fexec go build -o $pkgname || Fdie
	Fexerel usr/bin/$pkgname
}

# optimization OK
