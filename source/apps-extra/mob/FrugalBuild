pkgname=mob
pkgver=5.1.1
pkgrel=1
pkgdesc='Tool for smooth git handover.'
archs=('x86_64')
groups=('apps-extra')
url='https://mob.sh'
depends=('glibc')
makedepends=('go')
_F_github_tag_v=y
_F_github_author=remotemobprogramming
Finclude github
sha1sums=('93b37ac8489afe0c2af11ed7c8c30cbe6915c85d')

build() {
	Fcd
	Fexec go build -o $pkgname || Fdie
	Fexerel usr/bin/$pkgname
}

# optimization OK
