pkgname=texi2html
pkgver=1.64
pkgrel=1
pkgdesc="Texi2html is a Perl script that converts Texinfo to HTML."
url="http://www.mathematik.uni-kl.de/~obachman/Texi2html/"
depends=('perl')
groups=('apps-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url | grep -m1 tar.gz | sed 's/.*l-\(.*\).ta.*/\1/'"
source=($url/Distrib/$pkgname-$pkgver.tar.gz)
sha1sums=('973b95dcc06fcdae4929a4a7a4ee7f51b568f84c')

