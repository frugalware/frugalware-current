pkgver=3.0
pkgrel=1
pkgdesc="A terminal multiplexer"
url="http://sourceforge.net/projects/tmux/"
depends=('ncurses>=6.0-12' 'libevent>=2.1.8')
_F_github_tag="y"
_F_github_grepv="to-merge"
Finclude github
groups=('apps-extra')
archs=('x86_64')
sha1sums=('364cc6a99c57e7b434aae3adb4b757fb2daf9d63')

build() {

	Fbuild
	Finstallrel 0644 /usr/share/$pkgname/example_tmux.conf
}

