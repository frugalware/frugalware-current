-					nabbrevs=($nabbrevs $s)
-				else
-					nabbrevs=($nabbrevs $insts)
-				fi
-			done
-			fmri_abbrevs=( $nabbrevs )
-		fi
-
-		# After playing with _multi_parts, I'm not sure it's actually
-		# that useful.
-		# _wanted fmri expl "full or unambiguously abbreviated FMRIs" \
-		# 	_multi_parts -i / fmri_abbrevs
