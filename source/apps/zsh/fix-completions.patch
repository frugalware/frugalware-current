-
-(( $+functions[_bsd_pkg_pkgs_and_files] )) ||
-_bsd_pkg_pkgs_and_files() {
-  local ret=1
-
-  if (( $words[(I)-*F*] )); then
-    _files "$@" && ret=0
-  else
-    _bsd_pkg_pkgs "$@" && ret=0
-  fi
-
-  return ret
-}
-
-_bsd_pkg() {
