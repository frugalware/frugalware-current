pkgname=sof-firmware
pkgver=1.6.1
pkgrel=1
pkgdesc="Sound Open Firmware"
archs=('x86_64')
groups=('base')
depends=('glibc')
url="https://www.sofproject.org/"
options=('nostrip')
_F_github_author=thesofproject
_F_github_name=sof-bin
_F_github_tag_v=y
Finclude github
sha1sums=('c0cd146bba17fb19e397720468c1451fef26a06b')
_F_cd_path="sof-bin-$pkgver"

build() {
	Fcd
	Fmkdir lib/firmware
  	ROOT="$Fdestdir/" SOF_VERSION=v$pkgver ./go.sh || Fdie
}


# optimization OK
