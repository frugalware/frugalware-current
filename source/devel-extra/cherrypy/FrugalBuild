pkgname=cherrypy
pkgver=3.1.2
pkgrel=2
pkgdesc="A pythonic, object-oriented web development framework"
url="http://www.cherrypy.org"
depends=('python>=2.7')
groups=('devel-extra')
archs=('i686' 'x86_64')
_F_archive_name="CherryPy"
up2date="Flasttar $url/wiki/CherryPyDownload"
source=(http://download.cherrypy.org/$pkgname/$pkgver/$_F_archive_name-$pkgver.tar.gz)
sha1sums=('a94aedfd0e675858dbcc32dd250c23d285ee9b88')

build()	{
	Fbuild
	Fdocrel $pkgname/LICENSE.txt
}

