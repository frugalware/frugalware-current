# Compiling Time: 0.01 SBU
# Maintainer: Miklos Vajna <vmiklos@frugalware.org>

pkgname=docutils
pkgver=0.5
pkgrel=1
pkgdesc="Documentation Utilities"
Finclude sourceforge
depends=('setuptools')
groups=('devel-extra')
archs=('i686')
sha1sums=('70ca2dd94969ddf76217e5fe907b0b23f16cc199')

build()
{
	Fbuild
	cd $Fdestdir/usr/bin
	for i in *.py
	do
		ln -s $i `basename $i .py`
	done
}
