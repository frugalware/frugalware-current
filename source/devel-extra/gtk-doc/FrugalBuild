pkgname=gtk-doc
pkgver=1.28
pkgrel=2
pkgdesc="Used to document the public API of libraries"
url="http://www.gnome.org/"
depends=('openjade' 'docbook-xsl' 'source-highlight' 'perl>=5.28.2' 'python-six')
makedepends=('pkgconfig' 'itstool' 'yelp-tools')
groups=('devel-extra')
archs=('x86_64')
_F_gnome_ext=".tar.xz"
_F_gnome_devel="n"
Finclude gnome
sha1sums=('f48311fd3def5b393ebb32d5480a61ec6dfd2809')

build(){
	Fcd
	Fsed '$(datadir)/pkgconfig' '$(libdir)/pkgconfig' Makefile.{am,in}
	Fbuild
}


# optimization OK
