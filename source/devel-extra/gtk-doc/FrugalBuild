pkgname=gtk-doc
pkgver=1.32
pkgrel=1
pkgdesc="Used to document the public API of libraries"
url="http://www.gnome.org/"
depends=('openjade' 'docbook-xsl' 'source-highlight' 'perl>=5.28.2' 'python-six')
makedepends=('pkgconfig' 'itstool')
groups=('devel-extra')
archs=('x86_64')
_F_gnome_ext=".tar.xz"
_F_gnome_devel="n"
Finclude gnome
sha1sums=('36eb741d2f2e7791da37aa34dafea6d0506c9c45')

build(){
	Fcd
	Fsed '$(datadir)/pkgconfig' '$(libdir)/pkgconfig' Makefile.{am,in}
	Fbuild
}


# optimization OK
