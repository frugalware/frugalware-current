pkgname=gyp
pkgver=20220912.g9d09418
pkgrel=4
pkgdesc="GYP can Generate Your Projects."
url="https://chromium.googlesource.com/external/gyp"
groups=('devel-extra')
archs=('x86_64')
depends=('ninja')
makedepends+=('python3-setuptools')

_F_scm_type="git"
_F_scm_url="$url"
_F_scm_want_up2date=y
Finclude scm

build() {
	Funpack_scm
	Fbuild
}




