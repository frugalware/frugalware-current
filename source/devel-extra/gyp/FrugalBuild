pkgname=gyp
pkgver=20191208.ge87d37d
pkgrel=1
pkgdesc="GYP can Generate Your Projects."
url="https://chromium.googlesource.com/external/gyp"
groups=('devel-extra')
archs=('x86_64')
depends=('python-setuptools' 'ninja')

_F_scm_type="git"
_F_scm_url="$url"
_F_scm_want_up2date=y
Finclude scm

build() {
	Funpack_scm
	Fbuild
}




