pkgname=haskell-hslua-aeson
pkgver=2.2.0
pkgrel=1
pkgdesc="Allow aeson data types to be used with Lua."
groups=('devel-extra')
archs=('x86_64')
depends=('haskell-aeson' 'haskell-hslua-marshalling')
_F_haskell_setup="Setup.hs"
makedepends=('uusi')
Finclude haskell
sha1sums=('ff047d6324330d34e2ef40fa47f9dce8f9fe0fed')


build() {
        Fcd
        Fexec gen-setup || Fdie
        Fbuild_haskell
}


# optimization OK
