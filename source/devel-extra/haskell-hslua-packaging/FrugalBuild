pkgrel=1
pkgdesc="Utilities to build Lua modules."
groups=('devel-extra')
archs=('x86_64')
depends=('haskell-hslua-objectorientation')
_F_haskell_setup="Setup.hs"
makedepends=('uusi')
Finclude haskell
sha1sums=('9d721a3c4ec2f1403235ea0c094bededc2ca82a3')

build() {
        Fcd
        Fexec gen-setup || Fdie
        Fbuild_haskell
}


