post_upgrade()
{

	KEYSTORE="/usr/lib/jvm/java-7-openjdk/jre/lib/security/cacerts"

	for i in `find /etc/ssl/certs ! -type d`
	do
 		filename=$(basename "$i")
 		aliasname="${filename%.*}" 
 		/usr/lib/jvm/java-7-openjdk/jre/bin/keytool -import -keystore $KEYSTORE -storepass changeit -noprompt -file $i -alias $aliasname
	done
	
}
