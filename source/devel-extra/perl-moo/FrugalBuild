pkgname=perl-moo
_F_perl_name=Moo
_F_perl_author=H/HA/HAARG
pkgver=2.004000
pkgdesc="Minimalist Object Orientation (with Moose compatiblity)"
archs=('x86_64')
Finclude perl
pkgrel=1
sha1sums=('7de067c51a601018fae1cadcb8c632abf287b2ef')

build() {
        Fcd $_F_cd_path
        Fbuild	TT_ACCEPT=y
        Frm /usr/lib/perl5/current

}

# optimization OK
