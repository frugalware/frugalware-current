# Compiling Time: 0.01 SBU
# Maintainer: CSÉCSY László <boobaa@frugalware.org>

_F_pecl_name="apc"
pkgver=3.0.19
pkgrel=1
pkgdesc="Framework for caching and optimizing PHP intermediate code"
depends=('php>=5.2.9')
Finclude pecl
archs=('i686' 'x86_64')
pkgrel=1
sha1sums=('58d90ab757e606c878be3449a738016a08061dfd')
up2date="lynx -dump http://pecl.php.net/package/$_F_pecl_name | grep '].*stable' -m1 | sed 's/.*APC-\(.*\)\.tgz.*/\1/'"

build() {
	Fcd APC-$pkgver
	Fbuildpecl
}

# optimization OK
