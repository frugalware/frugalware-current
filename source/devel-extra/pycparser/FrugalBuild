pkgname=pycparser
pkgver=2.21
pkgrel=1
pkgdesc="A complete C99 parsing library for python."
depends=('python-ply>=3.9')
groups=('devel-extra')
archs=('x86_64')
source+=(use-system-ply.patch)
sha1sums=('359c718c6ece361047f61846b9fa4ea0239576dc')
Finclude python pypi

build()
{
	Fbuild
	Frm /usr/lib/python$_F_python_ver/site-packages/$pkgname/ply
}

# optimization OK
