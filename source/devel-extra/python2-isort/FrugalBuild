pkgname=python2-isort
pkgver=5.10.1
pkgrel=1
pkgdesc="A Python utility / library to sort Python imports"
depends=('python')
makedepends=('python2-setuptools')
groups=('devel-extra')
archs=('x86_64')
_F_pypi_name=isort
Finclude pypi
