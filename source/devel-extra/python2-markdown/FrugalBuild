pkgdesc="Python implementation of Markdown."
depends=('python>=2.7')
makedepends+=('python-setuptools')
groups=('devel-extra')
archs=('x86_64')
_F_archive_name="Markdown"
_F_pypi_name='Markdown'
Finclude pypi
