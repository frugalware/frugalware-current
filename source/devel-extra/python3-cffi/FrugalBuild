pkgname=python3-cffi
_F_archive_name="cffi"
pkgver=1.12.3
pkgrel=1
pkgdesc="cryptography expose cryptographic recipes and primitives in a python module"
depends=('python3-pycparser>=2.16' 'python3' 'libffi>=3.2.1-2')
makedepends=('python3-setuptools')
groups=('devel-extra')
archs=('x86_64')
_F_pypi_name="cffi"
Finclude pypi
sha1sums=('ccc49cf31bc3f4248f45b9ec83685e4e8090a9fa')

build() {
	Fcd
	CFLAGS+=" -fno-strict-aliasing"
	Fexec python3 setup.py install --root=$Fdestdir || Fdie
}

# optimization OK
