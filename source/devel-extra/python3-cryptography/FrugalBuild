pkgname=python3-cryptography
pkgver=2.6.1
pkgrel=2
pkgdesc="Cryptography expose cryptographic recipes and primitives in a python module"
depends=('openssl>=1.1.1' 'python3>=3.6.8-4' 'python3-idna' 'python3-cffi' 'python3-asn1crypto' 'python3-six')
makedepends=('python3-pyasn1')
groups=('devel-extra')
archs=('x86_64')
_F_pypi_name="cryptography"
Finclude pypi
sha1sums=('0a14cbcdf1bc0262303912762d94cfd645e8c8de')

build() {
	Fcd
	CFLAGS+=" -fno-strict-aliasing"
	Fexec python3 setup.py install --root=$Fdestdir || Fdie
}

# optimization OK
