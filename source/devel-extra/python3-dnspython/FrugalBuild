_F_archive_name="dnspython"
pkgname=python3-dnspython
pkgver=2.3.0
pkgrel=2
pkgdesc="A DNS toolkit for Python"
depends=('python3-wheel')
makedepends=('python3-setuptools_scm')
groups=('devel-extra')
archs=('x86_64')
_F_python_version="python3"
_F_pypi_name="dnspython"
Finclude pypi
url="http://www.dnspython.org"
sha1sums=('e792904c215f5f50a299af082e6e3c390466e592')

build() {

	Fcd
	Fexec python3 setup.py install --root="$Fdestdir"
}

# optimization OK
