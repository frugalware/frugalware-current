# Compiling Time: 0 SBU
# Maintainer: DeX77 <dex77@frugalware.org>

pkgname=python3-fasteners
pkgver=0.17.3
pkgrel=1
pkgdesc="A python package that provides useful locks."
depends=('python3')
makedepends=('python3-setuptools' 'pyproject2setuppy')
groups=('devel-extra')
archs=('x86_64')
_F_pypi_name="fasteners"
_F_python_version=python3
Finclude pypi
sha1sums=('0667d50bf670ff6b445f39bebf06497bb71d1d60')

build() {
        Fcd
        Fexec python3 -m pyproject2setuppy.main build || Fdie
        Fexec python3 -m pyproject2setuppy.main install --root="$Fdestdir" --optimize=1 || Fdie
}

# optimization OK
