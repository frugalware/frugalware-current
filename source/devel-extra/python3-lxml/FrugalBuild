pkgname=python3-lxml
pkgver=4.9.1
pkgrel=3
pkgdesc="Easy-to-use library for working with XML and HTML in Python."
url="http://lxml.de/"
depends=('libxml2>=2.9.6' 'python3>=3.11' 'libxslt>=1.1.31')
makedepends=('python3-cython' 'python3-setuptools')
_F_github_tag="yes"
_F_github_ver="lxml-$pkgver"
_F_github_grepv="py27fix"
_F_github_author="lxml"
_F_github_name="lxml"
Finclude github
groups=('devel-extra')
archs=('x86_64')
sha1sums=('d8e2849be3230f7f44b86cb8fe4d2cd7a9880bad')

build()
{
	Fcd lxml-lxml-$pkgver ## lol
	Fpatchall
	Fexec python3 setup.py install --root=$Fdestdir --optimize=1 || Fdie
}


# optimization OK
