# Compiling Time: 0.13 SBU
# Maintainer: DeX77 <dex7@frugalware.org>

pkgname=python3-typing_extensions
pkgver=4.0.1
pkgrel=1
pkgdesc="Backported and Experimental Type Hints for Python 3.5+"
depends=('python3')
makedepends=('pyproject2setuppy')
groups=('devel-extra')
archs=('x86_64')
_F_pypi_name="typing-extensions"
_F_archive_name="typing_extensions"
Finclude pypi
sha1sums=('894fd138af80ebc02b78e36195846637d4e4b134')
_F_python_version=python3

build() {
        Fcd
        Fexec python3 -m pyproject2setuppy.main build || Fdie
        Fexec python3 -m pyproject2setuppy.main install --root="$Fdestdir" --optimize=1 || Fdie
}

# optimization OK
