# Compiling Time: 0.03 SBU
# Maintainer: Slown <slown@frugalware.org>

pkgname=ruby-bundler
pkgver=2.6.5
pkgrel=1
pkgdesc="Bundler manages an application's dependencies through its entire life, across many machines, systematically and repeatably."
url="http://bundler.io"
groups=('devel-extra')
archs=('x86_64')
_F_gem_name=${pkgname/ruby-/}
Finclude gem
_F_cd_path="."
sha1sums=('1566dbf2d14ea1530a39e791fae11dacc7a18058')

build() {
       Finstallgem
       Frm usr/bin/rake
}

# optimization OK
