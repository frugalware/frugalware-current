pkgname=xmpppy
pkgver=0.4.0
pkgrel=2
pkgdesc="Library for hanling xmpp with python."
depends=('python')
groups=('devel-extra')
archs=('i686' 'x86_64')
Finclude sourceforge
up2date="lynx -dump http://sourceforge.net/project/showfiles.php?group_id=97081|grep ']xmpppy '|sed 's/[^]]*][^]]*]\([^ ]*\) .*/\1/'"

build()
{
	Fcd
	rm Makefile
	Fbuild
	Fdocrel doc
}
sha1sums=('d88ca59dbfe444c25dc6c65d6361c08a59842dde')
