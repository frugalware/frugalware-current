pkgname=otf-texgyre
pkgver=2.501
pkgrel=2
pkgdesc="High quality open source opentype font collection resembling Helvetica, Times, Palatino, Courier, Gothic, Bookman, Schoolbook, and Chancery."
archs=('x86_64')
groups=('fonts-extra')
url="http://www.gust.org.pl/projects/e-foundry/tex-gyre"
depends=('font-util')
makedepends=('unzip')
source=("$url/whole/tg${pkgver/./_}otf.zip")
_F_archive_name="tg"
_F_archive_prefix="otf"
Fpkgversep=""
up2date="Flastarchive $url/whole .zip | sed 's/_/./'"
sha1sums=('826628fb1769b1aa06662fa778df305ac9f5c94d')
_F_fonts_subdir="OTF"
Finclude fonts

# optimization OK
