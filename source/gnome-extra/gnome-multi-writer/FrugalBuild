pkgname=gnome-multi-writer
pkgver=3.28.0
pkgrel=1
pkgdesc="Write an ISO file to multiple USB devices at once."
depends=('libgusb' 'udisks2' 'gtk+3>=3.22.21' 'libcanberra-gtk')
makedepends=('itstool')
groups=('gnome-extra' 'gnome-apps')
archs=('x86_64')
_F_gnome_glib="y"
_F_gnome_desktop="y"
_F_gnome_iconcache="y"
_F_gnome_devel="n"
_F_gnome_ext=".tar.xz"
Finclude gnome gnome-scriptlet meson
sha1sums=('8a057ff7cc068fe6f85992c7c46061eb838e4c2e')

build() {
	Meson_build
	Fbuild_gnome_scriptlet
}

# optimization OK
