pkgrel=1
pkgdesc="A font viewer utility for GNOME"
depends=('gtk+3>=3.22.29' 'gnome-desktop>=3.28.0')
makedepends=('intltool' 'itstool')
groups=('gnome')
archs=("x86_64")
_F_gnome_desktop="y"
_F_gnome_glib="y"
_F_gnome_ext=".tar.xz"
_F_gnome_devel="n"
Finclude gnome gnome-scriptlet meson
sha1sums=('d726c55dc9a89a5031cc7fce9147dfda678596ac')

build() {
	Meson_build
	Fbuild_gnome_scriptlet
}
