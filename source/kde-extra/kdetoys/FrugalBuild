pkgrel=1
pkgdesc="KDE Toys"
Finclude kde
depends=("kdebase-workspace>=${_F_kde_ver}")
makedepends=("${makedepends[@]}" "kdebase-workspace-compiletime>=${_F_kde_ver}" "ksgmltools2>=${_F_kde_ver}" \
    "docbook-xsl" "libdbusmenu-qt>=0.8.0")
groups=('kde-extra')
archs=('i686' 'x86_64')

subpkgs=("${subpkgs[@]}" "$pkgname-docs")
subdescs=("${subdescs[@]}" "$pkgname Documentation")
subdepends=("${subdepends[@]}" "")
subgroups=("${subgroups[@]}" "docs-extra kde-docs")
subarchs=("${subarchs[@]}" "i686 x86_64")

