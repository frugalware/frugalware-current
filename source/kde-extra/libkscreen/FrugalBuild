options+=('asneeded')

pkgname=libkscreen
pkgver=1.0.2
pkgrel=1
pkgdesc="KDE's screen management library."
groups=('kde-extra')
archs=('i686' 'x86_64')
_F_kde_dirname="stable/$pkgname/$pkgver/src"
Finclude kde
depends=("libkdeui>=$_F_kde_ver" 'qjson')
makedepends=("${makedepends[@]}" "kdelibs>=$_F_kde_ver" "kdelibs-compiletime>=$_F_kde_ver")
up2date="Flastverdir $_F_kde_mirror/stable/$pkgname"
sha1sums=('14c51818fa4e22408e3bc5af124eeff114d4c877')

# optimization OK
