options+=('asneeded')

pkgname=mobipocket
pkgrel=1
pkgdesc="A collection of plugins to handle mobipocket files."
groups=('kde-extra' 'kdegraphics')
archs=('i686' 'x86_64')
_F_kde_name=kdegraphics-mobipocket
Finclude kde
depends=("okular>=$_F_kde_ver")
makedepends=("${makedepends[@]}" "kdebase-workspace-compiletime>=$_F_kde_ver" "kdelibs>=$_F_kde_ver" "kdelibs-compiletime>=$_F_kde_ver")

# optimization OK
