pkgname=qtruby
pkgrel=2
pkgdesc="Ruby bindings for the Qt libraries."
groups=('kde-extra')
archs=('i686' 'x86_64')
Finclude kde
depends=("smokeqt>=$_F_kde_ver" 'ruby>=1.9.2')

build() {
	unset MAKEFLAGS
	unset _F_KDE_LD_FLAGS
	KDE_build
}

# optimization OK
