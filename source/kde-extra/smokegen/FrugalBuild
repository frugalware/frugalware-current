options+=('asneeded')

pkgname=smokegen
pkgrel=1
pkgdesc="A general purpose C++ parser with a plugin infrastructure."
groups=('kde-extra')
archs=('i686' 'x86_64')
Finclude kde
depends=("libqtxml>=$_F_kde_qtver")

build() {
	unset MAKEFLAGS
	unset _F_KDE_LD_FLAGS
	KDE_build
}

# optimization OK
