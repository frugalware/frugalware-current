pkgname=kdevelop
pkgver=3.2.2
pkgrel=3
pkgdesc="The KDevelop integrated C/C++ application development environment"
pkgurl="ftp://ftp.kde.org/pub/kde/stable/3.4.2/src"
url="http://www.kde.org"
groups=('kde' 'kde-core')
archs=('i686' 'x86_64')
depends=('kdebase=3.4.2')
up2date="lynx -dump http://www.kde.org/download/ |grep kdevelop|sed -n '1 p'|tr -s ' '|cut -d ' ' -f 2|cut -d - -f 2"
source=($pkgurl/$pkgname-$pkgver.tar.bz2)
md5sums=('6d788b8ec89ea9b19f5c281a574b48fe')

build()
{
	Fbuild --disable-debug
	# Not needed...
	Frm /usr/kdevbdb
}

