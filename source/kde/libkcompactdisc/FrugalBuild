Finclude kde
depends=("${depends[@]}" 'phonon>=4.6.0' "libsolid>=$_F_kde_ver" 'libpulse>=1.1')
makedepends=("${makedepends[@]}" "kdelibs-compiletime>=$_F_kde_ver" "kdelibs>=$_F_kde_ver")

