# Maintainer: Melko <melko@frugalware.org>

options+=('asneeded')

pkgname=oxygen-icons
pkgrel=1
pkgdesc="The Oxygen Icon Theme"
groups=('kde')
archs=('i686' 'x86_64')
depends=()
replaces=('nuvola')
url='http://www.oxygen-icons.org/'
license=('LGPL')
Finclude kde

build() {
	KDE_build
	Fmkdir usr/share/kde/icons
	Fmv usr/share/icons/* usr/share/kde/icons/
}
