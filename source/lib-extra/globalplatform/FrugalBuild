# Compiling Time: 0.06 SBU
# Maintainer: DeX77 <dex77@frugalware.org>

pkgname=globalplatform
pkgver=6.0.0
pkgrel=5
pkgdesc="GlobalPlatform libraries"
archs=('x86_64')
groups=('lib-extra')
depends=('glibc' 'pcsc-lite' 'openssl>=1.1')
_F_scm_type="subversion"
_F_scm_url="https://svn.code.sf.net/p/globalplatform/code/trunk"
Finclude sourceforge scm cmake
unset source

build() {
	Funpack_scm
	Fcd
	Fexec cd $pkgname || Fdie
	CMake_build
}

# optimization OK
