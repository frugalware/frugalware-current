# Compiling Time: 0.06 SBU
# Maintainer: DeX77 <dex77@frugalware.org>

pkgname=gppcscconnectionplugin
pkgver=1.1.0
pkgrel=4
pkgdesc="GlobalPlatform pcsc connection plugin"
archs=('x86_64')
groups=('lib-extra')
depends=('globalplatform>=6.0.0')
_F_sourceforge_dirname='globalplatform'
_F_scm_type="subversion"
_F_scm_url="https://svn.code.sf.net/p/globalplatform/code/trunk"
Finclude sourceforge scm cmake
unset source

build() {
        Funpack_scm
        Fcd
        Fexec cd $pkgname || Fdie
        CMake_build
}
# optimization OK
