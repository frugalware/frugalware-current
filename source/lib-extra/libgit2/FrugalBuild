options+=('asneeded')

pkgname=libgit2
pkgver=0.20.0
pkgrel=1
pkgdesc="portable, pure C implementation of the Git core methods"
archs=('i686' 'x86_64')
groups=('lib-extra')
depends=()
_F_github_sep=""
_F_github_ext=""
_F_github_ver="v$pkgver"
_F_github_tag="y"
Finclude github cmake
up2date="lynx -dump -listonly https://github.com/libgit2/libgit2/tags|grep -m1 .tar.gz|sed 's/.*v\(.*\).tar.gz/\1/'"
sha1sums=('0b2467215591323486188f3569abdadc962e53e3')
_F_cd_path="$pkgname-$pkgver"

# optimization OK
