pkgver=1.1.6
pkgrel=2
pkgdesc="A high-level API allowing the application programmer to construct and inject network packets."
url="http://www.packetfactory.net/libnet/"
depends=('glibc>=2.24-4')
groups=('lib-extra')
archs=('i686' 'x86_64')
_F_sourceforge_dirname="libnet-dev"
_F_archive_grepv="rc"
Finclude sourceforge
sha1sums=('dffff71c325584fdcf99b80567b60f8ad985e34c')
if [ "$CARCH" == "x86_64" ] ; then
    CFLAGS="$CFLAGS -fPIC"
fi

# optimization OK
