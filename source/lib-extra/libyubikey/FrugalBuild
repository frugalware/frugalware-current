# Compiling Time: 0.42 SBU

pkgname=libyubikey
pkgver=1.13
pkgrel=1
pkgdesc='Yubico YubiKey C library'
archs=('i686' 'x86_64')
groups=('lib-extra')
depends=('curl' 'libusb')
makedepends+=('asciidoc' 'docbook-xsl')
_F_github_author="Yubico"
_F_github_name="yubico-c"
_F_github_ver="$pkgname-$pkgver"
_F_github_tag="y"
Finclude github
options=('scriptlet')
up2date="$up2date | cut -d '_' -f 2"
sha1sums=('6d305404b10def9fc35ad395c9e1705f888cb9d2')

build() {
	Fcd
	Fautoreconf
	Fbuild
}

# optimization OK
