# Maintainer: DeX77 <dex77@frugalware.org>
# Compiling Time: 0.42 SBU

pkgname=libyubikey
pkgver=1.12
pkgrel=1
pkgdesc='Yubico YubiKey C library'
archs=('i686' 'x86_64')
groups=('lib-extra')
depends=('curl' 'libusb')
makedepends+=('asciidoc' 'docbook-xsl')
_F_github_author="Yubico"
_F_github_name="yubico-c"
_F_github_ver="$pkgname-$pkgver"
_F_github_tag="y"
Finclude github
options=('scriptlet')
up2date="$up2date | cut -d '_' -f 2"
sha1sums=('0187cc3a410071b25b19930f5d23d5ce2df2e21a')

build() {
	Fcd
	Fautoreconf
	Fbuild
}

# optimization OK
