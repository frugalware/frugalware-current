pkgrel=3
pkgdesc="A regular expressions library"
archs=('x86_64')
groups=('lib-extra' 'chroot-core')
depends=('glibc>=2.34')
_F_github_author="kkos"
_F_github_grepv="_rc"
_F_github_tag_v=y
Finclude github
options+=('noccache')
source=(https://github.com/kkos/oniguruma/releases/download/v${pkgver}${pkgextraver}/onig-${pkgver}.tar.gz)
