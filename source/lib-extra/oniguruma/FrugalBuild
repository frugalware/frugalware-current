pkgver=6.9.5
pkgextraver=_rev1
pkgrel=1
pkgdesc="A regular expressions library"
archs=('x86_64')
groups=('lib-extra')
depends=('glibc>=2.29-6')
_F_github_author="kkos"
_F_github_grepv="_rc"
Finclude github
up2date="$up2date | sed \"s/$pkgextraver//\""
options+=('noccache')
source=(https://github.com/kkos/oniguruma/releases/download/v${pkgver}${pkgextraver}/onig-${pkgver}${pkgextraver/_/-}.tar.gz)
sha1sums=('e0e6320303dfedf70b76f30da959d8dea61b72f2')
_F_cd_path="onig-${pkgver}"
_Fbuild_autoreconf="yes"

