pkgname=pexpect
pkgver=2.3
pkgrel=3
pkgdesc="A pure python expect-like module"
Finclude sourceforge
groups=('lib-extra')
archs=('i686' 'x86_64')
depends=('python')
sha1sums=('ee1e2770bfe49e7651bab78357179c28ed99a55b')

build(){
	Fbuild
	Fdocrel doc/*.html
	Fexerel examples/*.py usr/share/doc/$pkgname-$pkgver/examples/
}
