pkgname=pyside2
pkgver=5.14.2.2
_qtver=5.14.2
pkgrel=1
pkgdesc="The PySide project provides LGPL-licensed Python bindings for the Qt cross-platform application and UI framework."
url="https://wiki.qt.io/Qt_for_Python"
depends=('qt5-multimedia' 'qt5-tools' 'qt5-sensors' 'qt5-charts' 'qt5-webengine' 'qt5-websockets' \
	'qtspeech' 'qt5-3d' 'qt5-svg' 'qt5-script' 'qt5-scxml' 'qt5-x11extras' 'qt5-remoteobjects' \
	"python3-shiboken2>=$pkgver")
makedepends=('clang' 'qt5-tools-static' 'shiboken2')
groups=('lib-extra')
archs=('x86_64')
_F_pypi_name="PySide2"
_F_pypi_ext="-cp35.cp36.cp37.cp38-abi3-manylinux1_x86_64.whl"
_F_cmake_confopts="	-DBUILD_TESTS=OFF \
			-DPYTHON_EXECUTABLE=/usr/bin/python3"
Finclude pypi cmake
up2date="$up2date | sed \"s/_${_qtver}//\" "
source=("https://download.qt.io/official_releases/QtForPython/pyside2/PySide2-$pkgver-src/pyside-setup-opensource-src-${pkgver}.tar.xz")
sha1sums=('d8a0b7933f8ae41c0abda98b33cd8d35e9369cfe')
_F_cd_path="pyside-setup-opensource-src-${_qtver}/sources/pyside2"
options=('nolto' 'clang')
export CC=/usr/bin/clang
export CXX=/usr/bin/clang++

# optimization OK
