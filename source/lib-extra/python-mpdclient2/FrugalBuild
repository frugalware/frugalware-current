pkgname=python-mpdclient2
pkgver=0.11.1
pkgrel=2
pkgdesc="Python module for interfacing MPD"
url="http://www.musicpd.org/py-libmpdclient.shtml"
license="LGPL"
up2date="lynx -dump http://pygmy.berlios.de/files/?M=A |grep -m1 'py-lib'|sed 's/.tar.*//g'|sed 's/.*ent.//g'"
depends=('python' 'mpd')
archs=('i686' 'x86_64')
groups=('lib-extra')
source=(http://pygmy.berlios.de/files/py-libmpdclient-$pkgver.tar.bz2 )
sha1sums=('6f59718c85d6f6f60d49d75a3fcd9b8f9a5fd6f6')

build()
{
  Fcd py-libmpdclient-$pkgver
  python setup.py install --root=$Fdestdir
}

# optimalization OK --noarch
