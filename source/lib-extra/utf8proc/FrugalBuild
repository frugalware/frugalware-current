pkgname=utf8proc
pkgver=2.2.0
pkgrel=2
pkgdesc="A clean C library for processing UTF-8 Unicode data."
archs=('x86_64')
groups=('lib-extra')
depends=('glibc')
_F_github_author="JuliaLang"
_F_github_tag_v="yes"
Finclude github
source+=(libutf8proc.pc)
options+=('static')
_F_make_opts+=" prefix=/usr"
sha1sums=('476efd08dbff38c63f01bb9176905edb09384e63' \
          '0e0c8904f71f55fd88c83c604a0368e1a54977b5')

build() {

	Fbuild
	Ffile /usr/lib/pkgconfig/libutf8proc.pc
}
# optimization OK
