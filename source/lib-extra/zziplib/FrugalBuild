pkgname=zziplib
pkgver=0.13.62
pkgrel=2
pkgdesc="easily extract data from files archived in a single zip file"
depends=('zlib>=1.2.8-8')
groups=('lib-extra')
archs=('i686' 'x86_64')
_F_sourceforge_ext=.tar.bz2
Finclude sourceforge
up2date="$pkgver"
url="http://zziplib.sourceforge.net/"
sha1sums=('cf8b642abd9db618324a1b98cc71492a007cd687')

build() {

	Fcd
	Fautoreconf
	Fbuild
}

# optimization OK
