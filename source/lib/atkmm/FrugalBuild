pkgrel=9
pkgdesc="C++ interface for atk"
url="http://www.gnome.org/"
depends=('atk>=2.30.0' 'glibmm>=2.56.0-2')
groups=('lib')
archs=("x86_64")
_F_gnome_devel="n"
_F_gnome_ext=".tar.xz"
Finclude gnome
sha1sums=('e18e2145622d1ae9b32d0425aec1b4b74aeb1336')

subpkgs=("$pkgname-docs")
