pkgname=gd
pkgver=2.0.33
pkgrel=3
pkgdesc="an ansi C library to generate fancy images from programs"
url="http://www.boutell.com/gd/http"
depends=('fontconfig' 'libjpeg' 'libpng' 'freetype2')
groups=('lib')
archs=('i686' 'x86_64')
up2date="lynx -dump http://www.boutell.com/gd/| grep Stable|sed  -e 's|.*Version \(.*\) (.*|\1|g'"
source=($url/$pkgname-$pkgver.tar.gz gd-2.0.33-secfix.diff)
sha1sums=('89548efb01116a740c796a8df80de16d19856811' \
	  'cbb8431bc5ee54452eb968f35e687591f86f9f92')

# optimization OK
