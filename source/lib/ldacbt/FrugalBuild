pkgname=ldacbt
pkgver=1.1
pkgrel=4
pkgdesc="AOSP libldac dispatcher"
groups=('lib')
depends=('glibc>=2.34')
archs=('x86_64')
_F_github_author="EHfive"
_F_github_tag=y
_F_github_ver="$pkgver$pkgextraver"
_F_github_devel=yes
_F_scm_git_cloneopts=" --recurse-submodules"
Finclude github cmake

build(){
        Funpack_scm
        CMake_build
}


