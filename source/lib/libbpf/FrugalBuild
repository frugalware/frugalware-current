pkgname=libbpf
pkgver=1.4.6
pkgrel=1
pkgdesc="Library for loading eBPF programs and reading and manipulating eBPF objects from user-space"
groups=('lib')
archs=('x86_64')
depends=('elfutils')
_F_github_tag_v=y
Finclude github
sha1sums=('489490ff4491e74adfdb486683f3f30bdd4e20d7')
_F_cd_path="$pkgname-$pkgver/src"
_F_make_opts+=" LIBSUBDIR=lib"

build() {
	Fbuild
	Fmake install_headers
}

# optimization OK
