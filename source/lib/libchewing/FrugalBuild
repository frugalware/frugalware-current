pkgname=libchewing
pkgver=0.3.0
pkgrel=1
pkgdesc="Smart Chewing Library." ## kinda FIXME :-D
url="http://chewing.csie.net/"
depends=('glibc' 'check')
groups=('lib')
archs=('i686' 'x86_64')
up2date="lynx -dump $url/download/$pkgname|sort -n -r|head -n 1|sed 's/.*-\(.*\).t.*/\1/'"
source=($url/download/$pkgname/$pkgname-$pkgver.tar.gz)
sha1sums=('a3a29c663c05a055f83868251bb16363d0f8a1f5')

# optimization OK
