pkgrel=1
pkgdesc="A library for a new image format comparable to JPEG."
depends=('libpng>=1.6.25' 'libjpeg>=9a-2' 'libtiff>=4.0.6-2')
groups=('lib')
archs=("x86_64")
_F_github_name="$pkgname"
_F_github_author="webmproject"
_F_github_tag_v=y
Finclude github
sha1sums=('0db5d90bc3f6a8250772f660947caef76c15c4c1')
Fconfopts=" --enable-libwebpdemux "

