# Compiling time: ~0 minutes
# Maintainer: Janny <janny@vnet.hu>

pkgname=jdictionary-smartde-hu
pkgver=1.4
origname=SmartGerHun
extraver=1_4
pkgrel=1
pkgdesc="jDictionary SMART German - Hungarian plugin"
url="http://jdictionary.sourceforge.net"
license="LGPL"
depends=('jdictionary')
archs=('i686' 'x86_64')
groups=('locale-extra')
up2date="lynx -source -dump http://jdictionary.sourceforge.net/plugins.html|grep $origname|sed 's/.*-\(.*\)\.j.*/\1/;s/_/./'"
source=(http://west.dl.sourceforge.net/sourceforge/jdictionary/$origname-$extraver.jar)
md5sums=('59495128c72f48da7228ea8159af54ab')

build () {
	# install jDictionary plugins
	Ffile usr/share/jdictionary/Plugins/$origname-$extraver.jar
}

# optimalization ok - noarch

# vim: ft=sh
