pkgname=alsa-oss
pkgver=1.0.12
pkgrel=1
pkgdesc="OSS compatibility library"
url="http://www.alsa-project.org"
depends=('alsa-lib')
groups=('multimedia')
archs=('i686' 'x86_64')
up2date="lynx -dump $url |grep -om1 'alsa-oss-\(.*\)tar.bz2'|sed 's/.*oss-\(.*\).t.*/\1/'"
source=(ftp://ftp.alsa-project.org/pub/oss-lib/$pkgname-$pkgver.tar.bz2)

# optimization OK
