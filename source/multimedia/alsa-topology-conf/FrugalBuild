pkgname=alsa-topology-conf
pkgver=1.2.5
pkgrel=1
pkgdesc="ALSA topology configuration files"
url="http://www.alsa-project.org"
depends=('glibc>=2.29-6')
groups=('multimedia')
archs=("x86_64")
up2date="Flasttar $url"
source=(ftp://ftp.alsa-project.org/pub/lib/$pkgname-$pkgver.tar.bz2)
sha1sums=('8c1d225b9fe108797349e8d0cb6161271aab0f50')
_F_archive_ver="1.2.4.2.g15998"

build() {
	Fcd
	Fmkdir usr/share/alsa/topology/
	Fcprel topology/\* usr/share/alsa/topology/
}

# optimization ok
