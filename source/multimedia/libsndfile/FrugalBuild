pkgname=libsndfile
pkgver=1.0.28
pkgrel=7
pkgdesc="Libsndfile is a C library for reading and writing files containing sampled sound"
url="http://www.mega-nerd.com/libsndfile"
groups=('multimedia')
archs=("x86_64")
depends=('flac>=1.4.0' 'sqlite3>=3.9.2-4' 'alsa-lib>=1.1.2-2' 'libogg>=1.3.2-5' 'libvorbis>=1.3.5-5')
up2date="Flasttar $url/files"
source="$url/files/$pkgname-$pkgver.tar.gz"
sha1sums=('85aa967e19f6b9bf975601d79669025e5f8bc77d')
_F_cross32_simple_auto="yes"
Finclude cross32

# optimization OK
