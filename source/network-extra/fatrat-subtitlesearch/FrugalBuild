pkgname=fatrat-subtitlesearch
pkgver=1.2.0_beta1
pkgrel=2
pkgdesc="FatRat plugin for downloading subtitles from OpenSubtitles.com."
url="http://fatrat.dolezel.info/"
depends=('fatrat>=1.2.0_beta2-4')
makedepends=('qt4-linguist')
_F_cmake_confopts="-DWITH_EVERYTHING=ON"
Finclude cmake
groups=('network-extra' 'fatrat-plugins')
archs=('i686' 'x86_64')
#up2date="Flasttar http://www.dolezel.info/download/data/$pkgname"
up2date=$pkgver
source=(http://www.dolezel.info/download/data/$pkgname/$pkgname-$pkgver.tar.gz)
sha1sums=('130aa25648ced2fc7f5319b55f1e311c19a879df')


