pkgname=links
pkgver=2.26
pkgrel=1
pkgdesc="A web browser running in both graphics and text mode."
url="http://links.twibright.com/"
depends=('xz' 'bzip2' 'zlib>=1.2.12' 'openssl>=1.1.1')
groups=('network-extra')
archs=('x86_64')
_F_archive_grepv="pre"
up2date="Flasttar $url/download/"
source=($url/download/$pkgname-$pkgver.tar.bz2)
Fconfopts+=" --disable-graphics --without-x --without-fb"
sha1sums=('dbe6811c10b702657e959a94c843e3420a3dfd86')

