makedepends=('swig' 'perl>=5.28.2' 'tcl') ## well modules no need to be depends=()
groups=('network-extra')
archs=('x86_64')
_F_archive_grepv="alpha\|beta\|rc"
up2date="Flasttar $url/releases"
source=($url/releases/$pkgname-$pkgver.tar.gz)
