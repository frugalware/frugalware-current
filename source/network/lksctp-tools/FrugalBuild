pkgname=lksctp-tools
pkgver=1.0.20
pkgrel=1
pkgdesc="Stream Control Transmission Protocol (SCTP) is a reliable, message-oriented, multihomed transport protocol."
depends=('glibc>=2.34')
_F_github_author="sctp"
_F_github_tag_v=y
Finclude github
groups=('network')
archs=("x86_64")
sha1sums=('08ae42b9a57cfd6a60c1429a457536cc6286bb27')
options=('nolto')

build()
{
	Fcd
	Fpatchall
	Fexec ./bootstrap || Fdie
	Fmake
	Fmakeinstall
}


# optimization OK
