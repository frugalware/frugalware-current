# Compiling Time: 0.19 SBU
# Maintainer: VMiklos <vmiklos@frugalware.org>

pkgname=neon
pkgver=0.26.4
pkgrel=2
pkgdesc="An HTTP and WebDAV client library, with a C interface."
url="http://www.webdav.org/neon/"
depends=('heimdal>=1.0' 'expat' 'zlib')
groups=('network')
archs=('i686' 'x86_64')
up2date="lynx -dump $url |grep Source|sed 's/.*-\(.*\)\.t.*/\1/'"
source=($url/$pkgname-$pkgver.tar.gz)
options=('force')
Fconfopts="$Fconfopts --disable-debug --with-expat --with-ssl --enable-shared"
sha1sums=('0c005fe73573253888b0c6e839b18d24c52582a4')
# optimization OK
