pkgname=traceroute
pkgver=2.1.4
pkgrel=1
pkgdesc="Traces the route taken by packets over a TCP"
groups=('network')
archs=("x86_64")
depends=('glibc>=2.27-2')
Finclude sourceforge
sha1sums=('59b8bde8c0b2fe7a72f701f6418e564f7b5a7e44')

build()
{
	Fcd
	Fexec make CFLAGS="$CFLAGS" || Fdie
	Fmakeinstall
}

# optimization OK
