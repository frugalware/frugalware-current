pkgname=traceroute
pkgver=2.1.0
pkgrel=1
pkgdesc="Traces the route taken by packets over a TCP"
groups=('network')
archs=("x86_64")
depends=('glibc')
Finclude sourceforge
sha1sums=('bc5c6c8022187511be5665b3818d919be5987dcc')

build()
{
	Fcd
	Fexec make CFLAGS="$CFLAGS" || Fdie
	Fmakeinstall
}

# optimization OK
