pkgname=traceroute
pkgver=2.1.1
pkgrel=1
pkgdesc="Traces the route taken by packets over a TCP"
groups=('network')
archs=("x86_64")
depends=('glibc>=2.27-2')
Finclude sourceforge
sha1sums=('74cad59c5b698e9686913b501559203de94e4099')

build()
{
	Fcd
	Fexec make CFLAGS="$CFLAGS" || Fdie
	Fmakeinstall
}

# optimization OK
