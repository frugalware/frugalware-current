pkgname=traceroute
pkgver=2.1.2
pkgrel=1
pkgdesc="Traces the route taken by packets over a TCP"
groups=('network')
archs=("x86_64")
depends=('glibc>=2.27-2')
Finclude sourceforge
sha1sums=('63ea3386fd3b01c008699dbb5c10eceb27269e2e')

build()
{
	Fcd
	Fexec make CFLAGS="$CFLAGS" || Fdie
	Fmakeinstall
}

# optimization OK
