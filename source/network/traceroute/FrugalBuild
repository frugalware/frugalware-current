pkgname=traceroute
pkgver=2.1.3
pkgrel=1
pkgdesc="Traces the route taken by packets over a TCP"
groups=('network')
archs=("x86_64")
depends=('glibc>=2.27-2')
Finclude sourceforge
sha1sums=('0419282983867ff52191302a5b067364e6695d72')

build()
{
	Fcd
	Fexec make CFLAGS="$CFLAGS" || Fdie
	Fmakeinstall
}

# optimization OK
