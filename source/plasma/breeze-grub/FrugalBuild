pkgname=breeze-grub
pkgver=5.13.0
pkgrel=1
pkgdesc="Breeze theme for GRUB"
groups=('plasma')
archs=("x86_64")
_F_kde_project="plasma"
Finclude kf5
rodepends=('grub2')
sha1sums=('d50361900de85b911414dea51c06a00f565a45d0')

build(){
	Fcd
	Fmkdir usr/share/grub/themes/
	Fcprel breeze usr/share/grub/themes/
}

# optimization OK
