pkgname=breeze-grub
pkgver=5.17.3
pkgrel=1
pkgdesc="Breeze theme for GRUB"
groups=('plasma')
archs=("x86_64")
_F_kde_project="plasma"
Finclude kf5
rodepends=('grub2')
sha1sums=('844955a387890a2079040ff8b037c003ac3a4df7')

build(){
	Fcd
	Fmkdir usr/share/grub/themes/
	Fcprel breeze usr/share/grub/themes/
}

# optimization OK
