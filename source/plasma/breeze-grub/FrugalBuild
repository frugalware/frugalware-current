pkgname=breeze-grub
pkgver=5.18.4.1
pkgrel=2
pkgdesc="Breeze theme for GRUB"
groups=('plasma')
archs=("x86_64")
_F_kde_project="plasma"
Finclude kf5
rodepends=('grub2')
sha1sums=('b0966554da67ff985502c452a88ecea061e31204')

build(){
	Fcd
	Fmkdir usr/share/grub/themes/
	Fcprel breeze usr/share/grub/themes/
}

# optimization OK
