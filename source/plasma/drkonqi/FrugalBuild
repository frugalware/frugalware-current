pkgname=drkonqi
pkgver=5.26.2
pkgrel=2
pkgdesc="The KDE Crash Handler."
groups=('plasma')
archs=("x86_64")
_F_kde_project="plasma"
Finclude kf5
depends=("kxmlrpcclient>=$_F_kf5_full" "kidletime>=$_F_kf5_full" \
	"syntax-highlighting>=$_F_kf5_full" "kdeclarative>=$_F_kf5_full" \
	'pcre2' "kuserfeedback")
makedepends+=("kdoctools>=$_F_kf5_full" 'systemd-devel')
sha1sums=('5865d9442988db258cf3e18c179793466b0c0615')

build() {
	KDE_build
	Fmkdir lib
	Fmv usr/lib/systemd lib/systemd	
}
# optimization OK
