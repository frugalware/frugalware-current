pkgrel=2
pkgdesc="Config for GTK within KDE"
groups=('plasma')
archs=("x86_64")
_F_kde_project="plasma"
Finclude  kf5
sha1sums=('802d0f22054145807a8a667e21b244020cb3731a')
depends=("knewstuff>=$_F_kf5_full" "kcmutils>=$_F_kf5_full" "qt5-svg" 'harfbuzz' 'gsettings-desktop-schemas')
# Those two are actualy TRUE depends but pull in hell of stuff
makedepends+=('gtk+2' 'gtk+3' )

CFLAGS="${CFLAGS} -isystem /usr/include/harfbuzz"
CXXFLAGS="${CXXFLAGS} -isystem /usr/include/harfbuzz"

# optimization OK
