groups=('plasma')
archs=("x86_64")
_F_kde_project="plasma"
Finclude kf5
sha1sums=('979c1e9f0731a39ed8d9acd9482ca44364aff5d6')
depends=("kwallet5>=$_F_kf5_full" 'pam>=1.1.8-4' 'libgcrypt>=1.7.3-2' 'socat>=1.7.3.1-2')

build()
{

	KDE_build
	Fmkdir lib/
	Fmv usr/lib/security lib/security
}

