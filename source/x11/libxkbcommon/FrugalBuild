groups=('x11')
archs=('x86_64')
depends=('glibc>=2.29-6' 'libxcb>=1.12-2' 'xkeyboard-config>=2.18-2')
makedepends=('byacc>=20160606-2' 'x11-protos>=0.1-3' 'wayland-protocols' 'doxygen' 'bison')
Finclude meson
up2date="lynx -dump http://xkbcommon.org/ | grep 'tar.xz' | Flasttar"
