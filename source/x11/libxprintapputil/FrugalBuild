pkgname=libxprintapputil
realname=libXprintAppUtil
pkgver=1.0.1
pkgrel=1
pkgdesc="X.Org XprintAppUtil library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'libxp' 'libxprintutil')
makedepends=('pkgconfig' 'inputproto')
Finclude xorg

build() {
	Fcd libXprintAppUtil-$pkgver
	Fmake
	Fmakeinstall
}

# optimalization OK

