pkgname=libxprintutil
realname=libXprintUtil
pkgver=1.0.1
pkgrel=1
pkgdesc="X.Org XprintUtil library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'libxp' 'libxt')
makedepends=('pkgconfig' 'inputproto')
Finclude xorg

build() {
	Fcd libXprintUtil-$pkgver
	Fmake
	Fmakeinstall
}

# optimalization OK

