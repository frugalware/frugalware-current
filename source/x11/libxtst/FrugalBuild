pkgname=libxtst
realname=libXtst
pkgver=1.0.1
pkgrel=1
pkgdesc="X.Org Xtst library"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=('libx11' 'libxext' 'recordproto')
makedepends=('pkgconfig' 'inputproto')
Finclude xorg

build() {
	Fcd libXtst-$pkgver
	Fmake
	Fmakeinstall
}

# optimalization OK

