pkgrel=2
pkgdesc="X.org X protocol descriptions for XCB"
url="http://xcb.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-proto')
depends=('libpthread-stubs')
archs=('x86_64')
options=('nostrip')
