pkgname=xkbcomp
pkgver=1.2.0
pkgrel=3
pkgdesc="X.Org xkbcomp application"
url="http://xorg.freedesktop.org"
groups=('x11' 'xorg-core' 'xorg-apps')
archs=('i686' 'x86_64' 'ppc')
depends=('libx11>=1.3' 'libxkbfile>=1.0.5')
makedepends=('inputproto>=1.99.902' 'util-macros>=1.2.1')
Finclude xorg
sha1sums=('67a10a1671fd1a0d6e09b4dfe93d0331af699f26')

build(){
	Fcd
	Fpatchall
	Fautoreconf
	Fmake
	Fmakeinstall
}

