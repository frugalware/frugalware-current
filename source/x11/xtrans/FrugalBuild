pkgname=xtrans
pkgver=1.4.0
pkgrel=3
pkgdesc="Abstract Network code for X"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('x86_64')
depends=()
Finclude xorg
makedepends=()
sha1sums=('b5d636c096a4ef3840b9d1dfef648c98a04a4dd0')

build()
{
	Fcd
	Fsed '$(datadir)/pkgconfig' '/usr/lib/pkgconfig' Makefile.{am,in}
	Fbuild
}
# optimization OK
