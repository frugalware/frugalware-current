pkgname=xtrans
pkgver=1.3.5
pkgrel=4
pkgdesc="Abstract Network code for X"
groups=('x11' 'xorg-core' 'xorg-libs')
archs=('i686' 'x86_64')
depends=()
Finclude xorg
sha1sums=('2d3ae1839d841f568bc481c6116af7d2a9f9ba59')

build()
{
	Fcd
	Fsed '$(datadir)/pkgconfig' '/usr/lib/pkgconfig' Makefile.{am,in}
	Fbuild
}
# optimization OK
