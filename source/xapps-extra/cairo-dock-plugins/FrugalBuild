pkgname=cairo-dock-plugins
pkgver=2.1.3_3
pkgrel=1
pkgdesc="A set of plugins for cairo-dock."
url="http://www.glx-dock.org/"
depends=("cairo-dock>=$pkgver" 'vte>=0.20.5' 'libexif>=0.6.17' 'libxklavier>=3.9' \
	'alsa-lib>=1.0.20')
makedepends=('intltool')
groups=('xapps-extra')
options=('scriptlet')
archs=('i686' 'x86_64')
up2date="elinks -dump 'https://launchpad.net/cairo-dock-plug-ins' | Flasttar"
source=(http://launchpad.net/cairo-dock-plug-ins/2.1/2.1.3/+download/$pkgname-${pkgver//_/-}.tar.gz)
_F_cd_path="$pkgname-${pkgver//_/-}"
sha1sums=('2a5353a5b7b6a1ddd1edfaf7ac363bc4df2e45ed')

