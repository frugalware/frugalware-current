pkgname=clight-gui
pkgver=4.8
pkgrel=3
pkgdesc="Qt GUI for Clight"
archs=('x86_64')
groups=('xapps-extra')
_F_github_author=nullobsi
_F_github_devel=y
Finclude github cmake
depends=('clight' 'qt5-charts>=5.15.10')
makedepends+=('qt5-tools')

build() {
	Funpack_scm
	Fexec cd src || Fdie

	CMake_build
}

# optimization OK
