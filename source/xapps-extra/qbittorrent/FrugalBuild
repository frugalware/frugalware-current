pkgname=qbittorrent
pkgver=4.1.5
pkgrel=4
pkgdesc="A Bittorrent client."
depends=('libtorrent-rasterbar>=1.2.0' 'qt5-svg>=5.13.1' 'libboost>=1.70.0')
rodepends=('geoip' 'python')
makedepends=('boost>=1.60.0' 'qt5-tools>=5.13.1' 'x11-protos')
groups=('xapps-extra')
archs=('x86_64')
_F_gnome_iconcache="y"
_F_github_devel=yes
Finclude github
url="http://www.$pkgname.org/"

build() {
	Funpack_scm
	Fmake
	Fexec make INSTALL_ROOT=$Fdestdir install || Fdie
}

# optimization OK
