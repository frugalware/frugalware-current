pkgname=simpleburn
pkgver=1.5.0
pkgrel=2
pkgdesc="SimpleBurn is a basic application for burning CDs and DVDs."
url="http://simpleburn.tuxfamily.org/"
urldl="http://download.tuxfamily.org/simpleburn/"
up2date="Flasttar $urldl"
source=("$urldl/$pkgname-$pkgver.tar.bz2")
depends=('libglade>=2.6.4-3' 'gtk+2>=2.20.0-2' 'cdrtools' 'hal')
options=('scriptlet')
groups=('xapps-extra')
archs=('i686' 'x86_64')
Finclude cmake
sha1sums=('82793d77a5c57ae16dcae39f42fdc4a891b93466')

# optimization OK
