	--disable-aleph \
	--enable-luatex \
	--with-clisp-runtime=default \
	--enable-xindy \
	--disable-xindy-rules \
	--disable-xindy-docs"
_F_make_opts+=" -j1"

subpkgs=("$pkgname-xindy"
        "$pkgname-bibtex-extra"
        "$pkgname-misc"
