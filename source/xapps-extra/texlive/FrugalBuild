## disabled by default since 1.2GB
: ${USE_DOCS="n"}
## 100 MB lang we don't really support
: ${USE_CJK="n"}
## another one
: ${USE_GREEK="n"}
## 300 MB extra fonts ?
## someone may want to split these in more packages ?
## until then .. disabled by default
: ${USE_EFONTS="y"}

pkgname=texlive
pkgver=2022.0
_relversion=20220321
pkgextraver="-source"
pkgrel=1
pkgdesc="A complete TeX distribution"
archs=('x86_64')
depends=('t1lib' 'gd>=2.1.1' 'ncurses>=6.0-3' 'poppler>=20.08.0' 'icu4c>=71.1' \
	'zlib>=1.2.12' 'graphite2' 'zziplib' 'libpaper' 'harfbuzz' 'libgs' 'mpfr' )
rodepends=('perl' 'texlive-kpathsea' 'texlive-ptexen' 'texlive-lua')
makedepends=('clisp' 'ffcall' 'x11-protos' 'lua-static' 'ghostscript')
url='http://tug.org/texlive/'
source=(http://ftp.math.utah.edu/pub/tex/historic/systems/texlive/2022/$pkgname-${_relversion}-source.tar.xz \
	http://ftp.math.utah.edu/pub/tex/historic/systems/texlive/2022/$pkgname-${_relversion}-texmf.tar.xz \
	http://ftp.math.utah.edu/pub/tex/historic/systems/texlive/2022/$pkgname-${_relversion}-extra.tar.xz \
	0002-normalize-all-updmap-path.patch \
	0003-fix-a2ping-invocation-and-shebang.patch \
	0004-remove-luazlib-zlib-header-check.patch \
	0005-utils-pmx-pmx-src-libf2c-f2c.h-remove-unnecesary-mac.patch \
	0006-latex2man-use-predictable-tmp.patch \
	texk-kpathsea.patch \
	texmf.cnf \
	texmfcnf.lua \
	09-texlive.conf)
up2date=" lynx --dump http://tug.org/svn/texlive/tags/ | grep tags/texlive- | Flastverdir | sed 's/texlive_//'"
groups=('xapps-extra')
options=('force')
_F_cd_path="texlive-${_relversion}-source"

conflicts=('tetex' 'tetex-kpathsea' "$pkgname-ptex")
provides=("${conflicts[@]}")
replaces=("${conflicts[@]}")

