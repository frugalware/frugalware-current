pkgname=gqview
pkgver=2.1.5
pkgrel=3
pkgdesc="An image browser"
depends=('gtk+2>=2.20.1')
groups=('xapps')
archs=('i686' 'x86_64' 'ppc')
Finclude sourceforge
url="http://gqview.sourceforge.net/"
options=('scriptlet')

build()
{
	Fsed 'if 1' 'if 0' src/main.c
	Fbuild
}

# optimization OK
sha1sums=('42fbbe4bcd8ad87a1b976cc8a4433750cac7b116')
