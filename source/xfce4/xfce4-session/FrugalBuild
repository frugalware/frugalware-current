# XFCE Build Environment - xfce4-session
# Last modified: Wed, 31 Aug 2005 12:35:50 +0200
# Build time: 0.40 SBU
# Maintainer: Krisztian VASAS <iron@ironiq.hu>

pkgname=xfce4-session
pkgver=4.2.2
pkgrel=2
pkgdesc="A desktop manager for Xfce4"
url="http://www.xfce.org/"
depends=('libxfcegui4' 'startup-notification' 'libxfce4mcs')
groups=('xfce4' 'xfce4-core')
archs=('i686' 'x86_64')
Finclude xfce4
source=($dlurl/$pkgname-$pkgver.tar.gz)
sha1sums=('c84ac42ef6069456740a4788a3ed14d9e86aeafd')

build() {
	[ -f /etc/profile.d/xfce4.sh ] && . /etc/profile.d/xfce4.sh
	Fbuild --sysconfdir=/etc || return 1
}

# optimalization OK

