# XFCE Build Environment - xfce4-systray
# Last modified: Wed, 31 Aug 2005 12:48:23 +0200
# Build time: 0.14 SBU
# Maintainer: Krisztian VASAS <iron@ironiq.hu>

pkgname=xfce4-systray
pkgver=4.2.2
pkgrel=2
pkgdesc="A systemtray plugin for Xfce4"
url="http://www.xfce.org/"
groups=('xfce4' 'xfce4-core')
archs=('i686' 'x86_64')
depends=('libxfcegui4' 'startup-notification' 'xfce4-panel')
Finclude xfce4
source=($dlurl/$pkgname-$pkgver.tar.gz)
sha1sums=('eb001bf07de9b29511a152bfb89edcb146f78dfd')

build() {
	[ -f /etc/profile.d/xfce4.sh ] && . /etc/profile.d/xfce4.sh
	Fbuild || return 1
}

# optimalization OK

