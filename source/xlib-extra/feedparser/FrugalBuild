pkgname=feedparser
pkgver=4.1
pkgrel=3
pkgdesc="Python RSS and Atom feeds parser."
url="http://www.feedparser.org/"
depends=('python')
groups=('xlib-extra')
archs=('i686' 'x86_64')
_F_sourceforge_ext=".zip"
Finclude sourceforge python
sha1sums=('3a7174e11c8c523ea461c9d47a80526ba29a6544')

build()
{
	Fdocrel docs
	mkdir build
	mv * build
	cd build
	Fbuild
}

# optimalization OK --noarch
