pkgname=open-sans
pkgver=1.10
pkgrel=1
pkgdesc="Open Sans is a humanist sans serif typeface designed by Steve Matteson, Type Director of Ascender Corp."
url="https://www.google.com/fonts/specimen/Open+Sans"
groups=('xlib-extra')
archs=('i686' 'x86_64')
_F_fonts_subdir="TTF"
Finclude fonts
up2date="$pkgver"
source=(http://pkgbuild.com/~alucryd/ttf-opensans-${pkgver}.zip)
sha1sums=('f7b309a9d345a3924fa4fd2c9f6465791de6552c')
options+=('force')
