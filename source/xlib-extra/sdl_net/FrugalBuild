pkgname=sdl_net
realname=SDL_net
pkgver=1.2.8
pkgrel=1
pkgdesc="A small sample cross-platform networking library"
url="http://www.libsdl.org/projects/SDL_net/"
depends=('sdl>=1.2.13-2' 'glibc')
groups=('xlib-extra')
archs=('i686' 'x86_64')
up2date="lynx -dump $url/release-1.2.html|grep -m1 'SDL_net-\(.*\).tar.gz'|sed 's/.*-\(.*\).t.*/\1/'"
source=($url/release/$realname-$pkgver.tar.gz)
_F_cd_path="$realname-$pkgver"
sha1sums=('fd393059fef8d9925dc20662baa3b25e02b8405d')

# optimization OK
