# Compiling Time: 0.24 SBU
# Maintainer: crazy <crazy@frugalware.org>

pkgname=wildmidi
pkgver=0.2.3.4
pkgrel=1
pkgdesc="A midi processing library and a midi player using the gus patch set."
depends=('eawpats' 'alsa-lib')
groups=('xlib-extra')
archs=('i686' 'x86_64')
options=('scriptlet')
Finclude sourceforge
Fconfopts+=" --disable-werror --disable-optimize --with-arch="""
sha1sums=('99ffeb54980a29c5aa44f4cb152ad37acbe877bc')

build()
{
	Fcd
	Fsed '-march=$arch' '' configure.ac
	Fautoreconf
	Fbuild
}
# optimization OK
