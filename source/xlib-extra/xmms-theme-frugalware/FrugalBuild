pkgname=xmms-theme-frugalware
pkgver=0.1
pkgrel=2
pkgdesc="Frugalware theme for xmms (made by Tyrry)"
url="http://frugalware.org"
rodepends=('xmms')
depends=()
groups=('xlib-extra')
archs=('i686' 'x86_64')
up2date="Flasttar http://ftp.frugalware.org/pub/other/artwork/xmms-theme/"
source=(http://ftp.frugalware.org/pub/other/artwork/xmms-theme//$pkgname-$pkgver.tar.bz2)
sha1sums=('c6f2f5fac0de653820056c1ec8446a3e05d3f9de')

build()
{
       Fmkdir usr/share/xmms/Skins
       Fcp frugalware usr/share/xmms/Skins
}
