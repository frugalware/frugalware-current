# Compiling Time: 0.11 SBU
# Maintainer: bouleetbil <bouleetbil@frogdev.info>

pkgname=pycairo
_F_archive_name=py2cairo
pkgver=1.10.0
pkgrel=3
pkgdesc="Python bindings for cairo"
url="http://cairographics.org"
## python3 support is there
depends=('python>=2.7.12-2' 'cairo>=1.14.6-4' 'libffi>=3.2.1-2')
makedepends=('x11-protos')
groups=('xlib')
archs=("x86_64")
options=('scriptlet')
up2date="Flasttar $url/releases/"
source=($url/releases/$_F_archive_name-$pkgver.tar.bz2)
sha1sums=('2efa8dfafbd6b8e492adaab07231556fec52d6eb')
_F_cd_path=$_F_archive_name-$pkgver


build()
{
	Fcd
	touch ChangeLog
	Fautoreconf
	Fbuild
}

# optimization OK
