pkgrel=2
depends=("qt5-base>=5.15.2")
Finclude qt5
options+=('ofast')

source=(revert-version-bump.patch)
_F_scm_type="git"
_F_scm_url="https://invent.kde.org/qt/qt/qtsvg.git"
_F_scm_tag="kde/5.15"
_F_scm_want_up2date=yes
Finclude scm

build()
{
       Funpack_scm
        build_qt5
}


