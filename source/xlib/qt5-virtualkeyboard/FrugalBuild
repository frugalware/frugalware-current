# Compiling Time: 64.56 SBU
# Maintainer: DeX77 <dex77@frugalware.org>

pkgname=qt5-virtualkeyboard
pkgver=5.7.0
pkgrel=3
depends=('qt5-declarative>=5.7.0-3' 'qt5-svg>=5.7.0-3' 'hunspell')
makedepends+=('x11-protos')
Finclude qt5
sha1sums=('893755ccf1d3133d88203ced814e11684eb2cb27')

build() {

	Fcd
	qmake-qt5 "CONFIG+=lang-all"
	make || Fdie
	make INSTALL_ROOT=$Fdestdir install || Fdie
}

# optimization OK

