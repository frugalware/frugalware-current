pkgname=joe-theme1
pkgver=0.1.0.g44b2d67
pkgrel=1
pkgdesc='concerned joe theme for sddm'
archs=('i686' 'x86_64')
groups=('xmultimedia-extra')
depends=('glibc')
rodepends=('sddm')
_F_github_author="Gaichu"
_F_github_devel=yes
Finclude github

build() {
        Funpack_scm
        Finstall 755 "$pkgname/*" /usr/share/sddm/themes/$pkgname/

}
# optimization OK
