depends=('qt5-base>=5.11.0-2' 'qt5-x11extras>=5.11.0-2' 'libuuid' 'libsamplerate' \
        'ffmpeg>=4.1' 'taglib' 'libmpcdec' 'libmad' 'qt5-multimedia>=5.11.0-2' \
	'libcdio-paranoia' 'wavpack' 'libmms' 'curl' 'libpulse>=9.0-4' 'libcdio>=2.1.0')
makedepends=('qt5-tools>=5.10.0' 'x11-protos')
groups=('xmultimedia-extra')
archs=('x86_64')
_F_gnome_iconcache="y"
_F_archive_grepv="svn"
Finclude cmake
up2date="Flasttar $url/files/"
source=($url/files/$pkgname-$pkgver.tar.bz2)
sha1sums=('d99e0bd98103ecb624c6275228301c6debabdd1b')

