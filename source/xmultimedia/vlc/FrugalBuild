_F_gnome_iconcache="y"
_F_cd_path="$pkgname$Fpkgversep$pkgver"
options+=('scriptlet')

Fconfopts+=" --disable-rpath  \
		--enable-nls \
		--enable-lirc \
		--enable-ncurses \
		--enable-bluray \
		--enable-sout \
		--enable-lua \
		--enable-vlm \
		--disable-notify \
		--disable-dc1394 \
		--enable-dvdread \
		--enable-dvdnav \
		--enable-v4l2 \
