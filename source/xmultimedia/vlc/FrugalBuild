		--enable-x264 \
		--enable-xvideo \
		--enable-freetype \
		--enable-libxml2 \
		--enable-svg \
		--enable-aa \
		--enable-alsa \
		--enable-skins2 \
		--enable-libgcrypt \
		--enable-vlc \
		--with-gnu-ld \
		--enable-dvbpsi \
		--enable-upnp \
		--disable-debug \
		--enable-freerdp \
		--enable-live555"

