pkgver=3.0.2
pkgrel=1
pkgdesc="The cross-platform media player and streaming server."
url="http://www.videolan.org/vlc/"

depends=('libva' 'alsa-lib' 'qt5-svg' 'qt5-x11extras' 'pcre2' 'fribidi')
